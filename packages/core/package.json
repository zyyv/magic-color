{
  "name": "@magic-color/core",
  "type": "module",
  "version": "2.2.0",
  "description": "Magic color core library",
  "author": "Chris <hizyyv@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/zyyv/magic-color#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zyyv/magic-color.git",
    "directory": "packages/core"
  },
  "bugs": "https://github.com/zyyv/magic-color/issues",
  "keywords": [
    "color",
    "colors",
    "magic-color"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.mts",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown -w",
    "attw": "attw --pack --config-path ../../.attw.json"
  },
  "dependencies": {
    "@magic-color/shared": "workspace:*",
    "@magic-color/transformer": "workspace:*"
  }
}
