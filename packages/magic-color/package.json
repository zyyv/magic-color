{
  "name": "magic-color",
  "type": "module",
  "version": "1.0.4",
  "description": "Magic color creator.",
  "author": "Chris <hizyyv@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/zyyv/magic-color#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zyyv/magic-color.git"
  },
  "bugs": "https://github.com/zyyv/magic-color/issues",
  "keywords": [
    "color",
    "colors",
    "magic-color"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub"
  },
  "dependencies": {
    "@magic-color/core": "workspace:*",
    "chroma-js": "^2.4.2"
  },
  "devDependencies": {
    "@types/chroma-js": "^2.4.4"
  }
